<?xml version="1.0" encoding="UTF-8"?>
<tmc-service-inc>
<pub id="RM21Q3EV">
<servcat id="33">
<name>TRANSMISSION</name>
<section id="1000Q_S000E" variety="S000E">
<name>A45DE AUTOMATIC TRANSMISSION</name>
<ttl id="1000Q_S000E_7A7ZB_T003P" variety="T003P">
<name>FLOOR SHIFT ASSEMBLY</name>
<para id="RM0000010NQ026X" category="A" type-id="80001" name-id="AT9CY-01" from="201202">
<name>REMOVAL</name>
<subpara id="RM0000010NQ026X_01" type-id="01" category="01">
<s-1 id="RM0000010NQ026X_01_0001" proc-id="RM21Q0EV__00000YP00000">
<ptxt>DISCONNECT CABLE FROM NEGATIVE BATTERY TERMINAL</ptxt>
<content1 releasenbr="1">
<atten2>
<ptxt>Wait at least 90 seconds after disconnecting the cable from the negative (-) battery terminal to prevent airbag activation.</ptxt>
</atten2>
</content1>
</s-1>
<s-1 id="RM0000010NQ026X_01_0009" proc-id="RM21Q0EV__000058J00000">
<ptxt>REMOVE CONSOLE BOX ASSEMBLY</ptxt>
<content1 releasenbr="2">
<ptxt>(See page <xref label="Seep01" href="RM000003NEC013X"/>)</ptxt>
</content1>
</s-1>
<s-1 id="RM0000010NQ026X_01_0006" proc-id="RM21Q0EV__00000YQ00000">
<ptxt>REMOVE FLOOR SHIFT ASSEMBLY</ptxt>
<content1 releasenbr="1">
<figure>
<graphic graphicname="G037854E01" width="2.775699831in" height="1.771723296in"/>
</figure>
<s2>
<ptxt>Disconnect the 2 connectors and clamp.</ptxt>
</s2>
<s2>
<ptxt>Move the shift lever to L.</ptxt>
</s2>
<s2>
<ptxt>Disconnect the transmission control cable.</ptxt>
</s2>
<s2>
<ptxt>Remove the 4 bolts.</ptxt>
<figure>
<graphic graphicname="G037855E01" width="2.775699831in" height="1.771723296in"/>
</figure>
</s2>
<s2>
<ptxt>Remove the floor shift.</ptxt>
</s2>
</content1>
</s-1>
</subpara>
</para>
</ttl>
</section>
</servcat>
</pub>
</tmc-service-inc>