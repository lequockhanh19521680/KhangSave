<results>
<result>NG</result>
<action-ci-right>GO TO STEP 18</action-ci-right>
<result-ci-down>OK</result-ci-down>
</results>
</descript-testgroup>
<descript-testgroup>
<testtitle>PROBLEM SYMPTOMS TABLE CHAPTER 3</testtitle>
<test1>
<ptxt>Refer to the problem symptoms table (see page <xref label="Seep13" href="RM000000W730XGX"/>).</ptxt>
</test1>
<results>
<result-ci-down>NG</result-ci-down>
</results>
</descript-testgroup>
<descript-testgroup>
<testtitle>PART INSPECTION</testtitle>
<results>
<result>NG</result>
<action-ci-right>GO TO STEP 20</action-ci-right>
<result-ci-down>OK</result-ci-down>
