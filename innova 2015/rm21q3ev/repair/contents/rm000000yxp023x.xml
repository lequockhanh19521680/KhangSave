<testtitle>RECHARGE OR REPLACE BATTERY</testtitle>
</testgrp>
<testgrp id="RM000000YXP023X_04_0011">
<testtitle>CONNECT CONNECTOR</testtitle>
</testgrp>
<testgrp id="RM000000YXP023X_04_0012">
<testtitle>REPLACE HARNESS AND CONNECTOR</testtitle>
</testgrp>
<testgrp id="RM000000YXP023X_04_0013">
<testtitle>REPLACE HARNESS AND CONNECTOR</testtitle>
