<?xml version="1.0" encoding="UTF-8"?>
<tmc-service-inc>
<pub id="RM21Q3EV">
<servcat id="40">
<name>SECURITIES</name>
<section id="1002N_S000U" variety="S000U">
<name>THEFT DETERRENT</name>
<ttl id="1002N_S000U_7A81M_T0060" variety="T0060">
<name>THEFT DETERRENT SYSTEM</name>
<para id="RM000001J8V00WX" category="J" type-id="30503" name-id="TD0B3-03" from="201202">
<dtccode/>
<dtcname>Driver Side Door Courtesy Switch Circuit</dtcname>
<subpara id="RM000001J8V00WX_01" type-id="60" category="03" proc-id="RM21Q0EV__000027R00000">
<name>DESCRIPTION</name>
<content5 releasenbr="1">
<ptxt>The theft warning ECU detects the condition of the front door courtesy light switch (driver side).</ptxt>
</content5>
</subpara>
<subpara id="RM000001J8V00WX_02" type-id="32" category="03" proc-id="RM21Q0EV__000027S00000">
<name>WIRING DIAGRAM</name>
<content5 releasenbr="1">
<figure>
<graphic graphicname="B119753E02" width="7.106578999in" height="3.779676365in"/>
</figure>
</content5>
</subpara>
<subpara id="RM000001J8V00WX_03" type-id="51" category="05">
<name>INSPECTION PROCEDURE</name>
</subpara>
<subpara id="RM000001J8V00WX_04" type-id="01" category="01">
<name>PROCEDURE</name>
<testgrp id="RM000001J8V00WX_04_0001" proc-id="RM21Q0EV__000027T00000">
<testtitle>CHECK COMBINATION METER ASSEMBLY</testtitle>
<content6 releasenbr="1">
<test1>
<ptxt>When the driver side door is opened / closed, check that the indicator in the combination meter operates normally.</ptxt>
<spec>
<title>OK</title>
<specitem>
<ptxt>Indicator operates normally.</ptxt>
</specitem>
</spec>
</test1>
</content6>
<res>
<down ref="RM000001J8V00WX_04_0002" fin="false">OK</down>
<right ref="RM000001J8V00WX_04_0003" fin="false">NG</right>
</res>
</testgrp>
<testgrp id="RM000001J8V00WX_04_0002" proc-id="RM21Q0EV__000027U00000">
<testtitle>CHECK WIRE HARNESS (COMBINATION METER - THEFT WARNING ECU)</testtitle>
<content6 releasenbr="1">
<test1>
<ptxt>Disconnect the C20 meter connector.</ptxt>
<figure>
<graphic graphicname="B119754E09" width="2.775699831in" height="3.779676365in"/>
</figure>
</test1>
<test1>
<ptxt>Disconnect the T20 ECU connector.</ptxt>
</test1>
<test1>
<ptxt>Measure the resistance of the wire harness side connectors.</ptxt>
<spec>
<title>Standard resistance</title>
<table>
<tgroup cols="2">
<colspec colname="COL1" colwidth="2.07in"/>
<colspec colname="COL2" colwidth="2.06in"/>
<thead>
<row>
<entry>
<ptxt>Tester Connection</ptxt>
</entry>
<entry>
<ptxt>Specified Condition</ptxt>
</entry>
</row>
</thead>
<tbody>
<row>
<entry>
<ptxt>C20-17 - T20-7 (CTY)</ptxt>
</entry>
<entry>
<ptxt>Below 1 立</ptxt>
</entry>
</row>
</tbody>
</tgroup>
</table>
</spec>
</test1>
<test1>
<ptxt>Measure the voltage of the wire harness side connectors.</ptxt>
<spec>
<title>Standard voltage</title>
<table>
<tgroup cols="2">
<colspec colname="COL1" colwidth="2.07in"/>
<colspec colname="COL2" colwidth="2.06in"/>
<thead>
<row>
<entry>
<ptxt>Tester Connection</ptxt>
</entry>
<entry>
<ptxt>Specified Condition</ptxt>
</entry>
</row>
</thead>
<tbody>
<row>
<entry>
<ptxt>C20-17 - Body ground</ptxt>
</entry>
<entry>
<ptxt>10 to 14 V</ptxt>
</entry>
</row>
</tbody>
</tgroup>
</table>
</spec>
</test1>
</content6>
<res>
<down ref="RM000001J8V00WX_04_0006" fin="true">OK</down>
<right ref="RM000001J8V00WX_04_0005" fin="true">NG</right>
</res>
</testgrp>
<testgrp id="RM000001J8V00WX_04_0003" proc-id="RM21Q0EV__000027V00000">
<testtitle>INSPECT FRONT DOOR COURTESY LIGHT SWITCH ASSEMBLY (FOR DRIVER SIDE)</testtitle>
<content6 releasenbr="1">
<test1>
<ptxt>Measure the resistance of the switch.</ptxt>
<figure>
<graphic graphicname="B119755E01" width="2.775699831in" height="1.771723296in"/>
</figure>
<spec>
<title>Standard resistance</title>
<table>
<tgroup cols="3">
<colspec colname="COL1" colwidth="1.38in"/>
<colspec colname="COL2" colwidth="1.38in"/>
<colspec colname="COL3" colwidth="1.37in"/>
<thead>
<row>
<entry>
<ptxt>Tester Connection</ptxt>
</entry>
<entry>
<ptxt>Switch Condition</ptxt>
</entry>
<entry>
<ptxt>Specified Condition</ptxt>
</entry>
</row>
</thead>
<tbody>
<row>
<entry>
<ptxt>1 - Body ground</ptxt>
</entry>
<entry>
<ptxt>Not pushed (ON)</ptxt>
</entry>
<entry>
<ptxt>Below 1 立</ptxt>
</entry>
</row>
<row>
<entry>
<ptxt>1 - Body ground</ptxt>
</entry>
<entry>
<ptxt>Pushed (OFF)</ptxt>
</entry>
<entry>
<ptxt>10 k立 or higher</ptxt>
</entry>
</row>
</tbody>
</tgroup>
</table>
</spec>
</test1>
</content6>
<res>
<down ref="RM000001J8V00WX_04_0004" fin="false">OK</down>
<right ref="RM000001J8V00WX_04_0007" fin="true">NG</right>
</res>
</testgrp>
<testgrp id="RM000001J8V00WX_04_0004" proc-id="RM21Q0EV__000027W00000">
<testtitle>CHECK WIRE HARNESS (COMBINATION METER - FRONT DOOR COURTESY LIGHT SWITCH (FOR DRIVER SIDE))</testtitle>
<content6 releasenbr="1">
<test1>
<ptxt>Disconnect the C20 meter connector.</ptxt>
<figure>
<graphic graphicname="B119756E11" width="2.775699831in" height="3.779676365in"/>
</figure>
</test1>
<test1>
<ptxt>Disconnect the D4 switch connector.</ptxt>
</test1>
<test1>
<ptxt>Measure the resistance of the wire harness side connectors.</ptxt>
<spec>
<title>Standard resistance</title>
<table>
<tgroup cols="2">
<colspec colname="COL1" colwidth="2.07in"/>
<colspec colname="COL2" colwidth="2.06in"/>
<thead>
<row>
<entry>
<ptxt>Tester Connection</ptxt>
</entry>
<entry>
<ptxt>Specified Condition</ptxt>
</entry>
</row>
</thead>
<tbody>
<row>
<entry>
<ptxt>C20-16 - D4-1</ptxt>
</entry>
<entry>
<ptxt>Below 1 立</ptxt>
</entry>
</row>
</tbody>
</tgroup>
</table>
</spec>
</test1>
</content6>
<res>
<down ref="RM000001J8V00WX_04_0009" fin="true">OK</down>
<right ref="RM000001J8V00WX_04_0008" fin="true">NG</right>
</res>
</testgrp>
<testgrp id="RM000001J8V00WX_04_0005">
<testtitle>REPAIR OR REPLACE HARNESS AND CONNECTOR</testtitle>
</testgrp>
<testgrp id="RM000001J8V00WX_04_0006">
<testtitle>PROCEED TO NEXT SUSPECTED AREA SHOWN IN PROBLEM SYMPTOMS TABLE</testtitle>
</testgrp>
<testgrp id="RM000001J8V00WX_04_0007">
<testtitle>REPLACE FRONT DOOR COURTESY LIGHT SWITCH</testtitle>
</testgrp>
<testgrp id="RM000001J8V00WX_04_0008">
<testtitle>REPAIR OR REPLACE HARNESS AND CONNECTOR</testtitle>
</testgrp>
<testgrp id="RM000001J8V00WX_04_0009">
<testtitle>REPLACE COMBINATION METER</testtitle>
</testgrp>
</subpara>
</para>
</ttl>
</section>
</servcat>
</pub>
</tmc-service-inc>